{
  "name": "js-di",
  "version": "0.0.333",
  "description": "Simple Dependency Injection for Javascript. Supports multiple containers with container inheritance",
  "keywords": [
    "dependency",
    "dependencies",
    "injection",
    "dependency injection",
    "DI"
  ],
  "main": "index.js",
  "scripts": {
    "test": "node jasmine-runner.js",
    "doc": "jsdoc src/injector.js -d ./docs",
    "build-readme": "jsdoc2md src/*.js --template README.hbs > README.md",
    "build": "mkdir -p dist && babel src/injector.js -o dist/injector.js"
  },
  "author": "Chris Bauer",
  "email": "cbauer@outlook.com",
  "url": "http://github.com/chrisbauer",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.6.0",
    "jasmine": "^2.4.1",
    "jasmine-spec-reporter": "^2.4.0",
    "jsdoc": "^3.4.0",
    "jsdoc-to-markdown": "^1.3.3"
  }
}
